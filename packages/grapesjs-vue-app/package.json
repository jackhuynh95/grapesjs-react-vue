  },
  "devDependencies" : {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-vue" : "^5.0.4",
    "copyfiles" : "^2.4.1",
    "dts-bundle-generator" : "^9.5.1",
    "rimraf" : "^6.0.1",
    "typescript" : "^5.2.2",
    "vite" : "^5.2.0",
