{
  "name": "@grapesjs/vue-app",
  "private": true,
  "version": "0.0.0",
  "files" : [ "dist" ],
  "main" : "./dist/@grapesjs/vue-app.umd.js",
  "module" : "./dist/@grapesjs/vue-app.es.js",
  "type" : "module",
  "exports" : { 
      "." : { 
          "import" : "./dist/@grapesjs/vue-app.js"
      },
      "./dist/index.css" : { 
          "import" : "./dist/style.css"
      },
      "./dist/" : { 
          "import" : "./dist/"
      }
  },
  "types" : "./dist/index.d.ts",
  "scripts" : { 
